          ]
        },
        {
          "code": "(p1)(5-6)",
          "display": "MM",
          "target": [
            {
              "equivalence": "equivalent",
              "code": "(p1)(6-7)",
              "display": "(p1)(6-7)"
            }
          ]
        },
        {
          "code": "(p1)(7-8)",
          "display": "DD",
          "target": [
            {
              "equivalence": "equivalent",
              "code": "(p1)(8)",
              "display": "= \"-\""
            }
          ]
        },
        {
          "code": "(p1)(7-8)",
          "display": "DD",
          "target": [
            {
              "equivalence": "equivalent",
              "code": "(p1)(9-10)",
              "display": "(p1)(9-10)"
            }
          ]
        }
      ]
    }
  ]
