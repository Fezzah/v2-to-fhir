  "definition": {
    "resource": [
      {
        "reference": {
          "reference": "StructureDefinition/accept-acknowledgement-type"
        },
        "name": "AcceptAcknowledgementType",
        "description": "The Accept Acknowledgement Type of the v2 message that was transformed into the FHIR message Bundle. This extension is\nonly relevant when performing a v2 to FHIR transformation. This extension corresponds to MSH-15 of the original message.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/access-restriction-range"
        },
        "name": "AccessRestrictionRange",
        "description": "The Access Restriction Date Range. This extension corresponds to ARV-6 of the original message.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/admission-level-of-care"
        },
        "name": "AdmissionLevelOfCare",
        "description": "Code indicating the acuity level assigned to the patient at the time of admission.\nThis extension corresponds to PV2-40 of the v2 PV2 segment.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/analysis-date-time"
        },
        "name": "AnalysisDateTime",
        "description": "The time stamp associated with generation of the analytical result by the instrument.\nThis extension corresponds to OBX-19 of the v2 OBX segment.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/application-acknowledgement-type"
        },
        "name": "ApplicationAcknowledgementType",
        "description": "The Application Acknowledgement Type of the v2 message that was transformed into the FHIR message Bundle. This extension is only relevant when performing a v2 to FHIR transformation.\nThis extension corresponds to MSH-15 of the original message.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/assigning-authority"
        },
        "name": "AssigningAuthority",
        "description": "Extensions supporting the coding of HD type stored in an Identifier\nThis extension corresponds to components of the v2 HD data type.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/binary-identifier"
        },
        "name": "BinaryIdentifier",
        "description": "Identifier issued by a vendor for the unique software version instance of the system originating the content of the transaction.\nThis extension corresponds to SFT-4 of the v2 SFT segment.",
        "exampleBoolean": false
      },
      {
