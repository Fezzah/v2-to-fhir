        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/duration"
        },
        "name": "Duration",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/episode-name"
        },
        "name": "EpisodeName",
        "description": "A brief user_Defined description of a Service Episode in or for which the encounter occurs.\nThis extension corresponds to PV1-53 of the v2 PV1 segment.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/estimated-length"
        },
        "name": "EstimatedLength",
        "description": "The estimated days of the inpatient stay.\nThis extension corresponds to PV2-10 of the v2 PV2 segment.\nIt is measured in units of days (http://unitsofmeasure.org/#d)",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/financial-class"
        },
        "name": "FinancialClass",
        "description": "The financial class(es) assigned to the patient for the purpose of identifying sources of reimbursement for the encounter.\nThis extension corresponds to PV1-20 of the v2 PV1 segment.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/fingerprint"
        },
        "name": "Fingerprint",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/identity-unknown"
        },
        "name": "IdentityUnknown",
        "description": "Indicator of whether or not the patient's identity is known.\nThis extension corresponds to PID-31 of the v2 PID segment.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/install-date"
        },
        "name": "InstallDate",
        "description": "The date the submitting software was installed at the sending site.\nThis extension corresponds to SFT-6 of the v2 SFT segment.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/job-status"
        },
        "name": "JobStatus",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/last-updated-facility"
        },
        "name": "LastUpdatedFacility",
        "description": "Reference which identifies the facility of the last update to a patient's identifying and demographic data.\nThis extension corresponds to PID-34 of the v2 PID segment",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/living-will"
        },
        "name": "LivingWill",
        "description": "A code indicating whether or not the patient has a living will and, if so, whether a copy of the living will is on file at the healthcare facility.\nIf the patient does not have a living will, the value of this field indicates whether the patient was provided information on living wills.\nThis extension corresponds to PD1-7 of the v2 PD1 segment.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/location"
        },
        "name": "Location",
        "description": "Location of the RelatedPerson acting in their role.\nThis extension corresponds to PRT-9 of the v2 PRT segment.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/location-period"
        },
        "name": "LocationPeriod",
        "description": "The time period that the location was relevant for the Observation.\nThis extension corresponds to PRT-11 and PRT-12 of the v2 PRT segment.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/name-type"
        },
        "name": "NameType",
        "description": "A code that represents the type of name of the Location i.e., XON.2 of the v2 XON data type.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/nature-of-abnormal-test"
        },
        "name": "NatureOfAbnormalTest",
        "description": "The nature of the abnormal test. For example, normal range values may be based on age, sex and/or race.\nThis extension corresponds to OBX-10 of the v2 OBX segment.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/note-type"
        },
        "name": "NoteType",
        "description": "The type of comment text in the Annotation (e.g. Patient Instructions or General Remark).\nThis extension corresponds to NTE-4 of the v2 NTE segment.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/original-code"
        },
        "name": "OriginalCode",
        "description": "The original code populating the CWE data type.\nRetention of the original code prevents the loss of information that may occur during translation.",
        "exampleBoolean": false
      },
      {
        "reference": {
          "reference": "StructureDefinition/parent-filler"
        },
        "name": "ParentFiller",
        "exampleBoolean": false
