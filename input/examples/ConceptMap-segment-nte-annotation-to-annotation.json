  "group": [
    {
      "element": [
        {
          "code": "NTE-3",
          "display": "Comment",
          "target": [
            {
              "equivalence": "equivalent",
              "code": "Annotation.text"
            }
          ]
        },
        {
          "code": "NTE-5",
          "display": "Entered By",
          "target": [
            {
              "equivalence": "equivalent",
              "code": "Annotation.authorReference(Practitioner)"
            }
          ]
        },
        {
          "code": "NTE-6",
          "display": "Entered Date/Time",
          "target": [
            {
              "equivalence": "equivalent",
              "code": "Annotation.time"
            }
          ]
        }
      ]
    }
  ]
