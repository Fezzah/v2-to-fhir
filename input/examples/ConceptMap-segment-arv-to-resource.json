              "equivalence": "equivalent",
              "code": "Resource.meta.security[3].display",
              "display": "Resource.meta.security[3].display"
            }
          ]
        },
        {
          "code": "ARV-7",
          "display": "Security Classification Tag",
          "target": [
            {
              "equivalence": "equivalent",
              "code": "Resource.meta.security[4]",
              "display": "Resource.meta.security[4]"
            }
          ]
        },
        {
          "code": "ARV-8",
          "display": "Security Handling Instructions",
          "target": [
            {
              "equivalence": "equivalent",
              "code": "Resource.meta.security[5]",
              "display": "Resource.meta.security[5]"
            }
