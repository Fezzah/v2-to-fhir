   <property>
      <code value="Low"/>
      <description value="Low value for cardinality"/>
      <type value="integer"/>
   </property>
   <property>
      <code value="High"/>
      <description value="High value for cardinality, set to 2147483647 if unbounded"/>
      <type value="integer"/>
   </property>
   <property>
      <code value="DataType"/>
      <description value="The V2 Datatype assigned to this model element"/>
      <type value="code"/>
   </property>
   <concept>
      <code value="There doesn't seem to be a single data type that includes all the concepts in CP The Money amount may also be represented as a Quantity, using the MoneyQuantity constraint sqty-1 Presumably the mappings would be Quantity"/>
      <property>
         <code value="Low"/>
         <valueInteger value="0"/>
      </property>
      <property>
         <code value="High"/>
         <valueInteger value="1"/>
      </property>
      <property>
         <code value="DataType"/>
      </property>
   </concept>
   <concept>
